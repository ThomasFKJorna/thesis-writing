@article{Knox2016,
  title = {Abstraction and Its {{Limits}}: {{Finding Space For Novel Explanation}}},
  shorttitle = {Abstraction and Its {{Limits}}},
  author = {Knox, Eleanor},
  date = {2016},
  journaltitle = {Noûs},
  volume = {50},
  number = {1},
  pages = {41--60},
  issn = {1468-0068},
  doi = {10.1111/nous.12120},
  abstract = {Several modern accounts of explanation acknowledge the importance of abstraction and idealization for our explanatory practice. However, once we allow a role for abstraction, questions remain. I ask whether the relation between explanations at different theoretical levels should be thought of wholly in terms of abstraction, and argue that changes of the quantities in terms of which we describe a system can lead to novel explanations that are not merely abstractions of some more detailed picture. I use the example of phase transitions as described by statistical mechanics and thermodynamics to illustrate this, and to demonstrate some details of the relationship between abstraction, idealization, and novel explanation.},

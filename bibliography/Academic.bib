@article{Bobzien2020,
  title = {Intuitionism and the {{Modal Logic}} of {{Vagueness}}},
  author = {Bobzien, Susanne and Rumfitt, Ian},
  date = {2020-04-01},
  journaltitle = {Journal of Philosophical Logic},
  shortjournal = {J Philos Logic},
